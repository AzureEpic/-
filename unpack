-- Scripts:

local function LRZU_fake_script() -- ImageButton.LocalScript 
	local script = Instance.new('LocalScript', ImageButton)

	script.Parent.MouseButton1Click:Connect(function()
	
	game:GetService("ReplicatedStorage").CharacterEvents.Struggle:FireServer(game:GetService("Players").LocalPlayer)
	
	
	
	local args = {
		[1] = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart,
		[2] = 0
	}
	
	game:GetService("ReplicatedStorage").CharacterEvents.RagdollRemote:FireServer(unpack(args))
	
	end)
end
coroutine.wrap(LRZU_fake_script)()
local function BSIIUFV_fake_script() -- AnchorButton.LocalScript 
	local script = Instance.new('LocalScript', AnchorButton)

	
	script.Parent.MouseButton1Click:Connect(function()
	
	for _, child in workspace:GetChildren() do
		if child:IsA("Model") and child.Name== "GrabParts" then
			local weld = child:FindFirstChild("GrabPart").WeldConstraint
			local Affected = weld.Part1
	
				local bpc = Affected:FindFirstChildOfClass("BodyPosition")
				if bpc then bpc:Destroy()
	
					local bgc = Affected:FindFirstChildOfClass("BodyGyroo")
					if bgc then bgc:Destroy()
	
						local hlc = Affected:FindFirstChildOfClass("Highlight")
						if hlc then hlc:Destroy()
	
	
	end end end
	
	
			
			local hl = Instance.new("Highlight")
			
			hl.Parent = Affected
			hl.FillColor = Color3.fromRGB(0,100,255)
			hl.OutlineColor = Color3.fromRGB(0,100,255)
			
			--anchor Affected with BodyPosition
			local bp = Instance.new("BodyPosition")
			bp.Parent = Affected
			bp.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
			bp.P = 10000
			bp.D = 1000
			bp.Position = Affected.Position
			bp.Parent = Affected
	
			local bg = Instance.new("BodyGyro")
			bg.Parent = Affected
			bg.MaxTorque = Vector3.new(math.huge,math.huge,math.huge)
			bg.P = 10
			
			
			
			
		end
	end
	end)
end
coroutine.wrap(BSIIUFV_fake_script)()
